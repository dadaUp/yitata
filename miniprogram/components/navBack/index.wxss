.nav-container {
  position: fixed;
  width: 100%;
  z-index: 99;
  top: 0;
  left: 0;
}

.back-icon {
  position: relative;
  width: 54rpx;
  height: 54rpx;
  float: left;
  margin: 16rpx;
  z-index: 2;
}

.title {
  width: 100%;
  box-sizing: border-box;
  margin-top: 8rpx;
  padding-left: 120rpx;
  padding-right: 120rpx;
  height: 32px;
  line-height: 32px;
  text-align: center;
  position: absolute;
  left: 0;
  z-index: 1;
  color: #333;
  font-size: 16px;
  text-overflow: ellipsis;
  overflow: hidden;
  white-space: nowrap;
}