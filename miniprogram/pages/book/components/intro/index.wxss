.intro {
  font-size: 30rpx;
  color: #191919;
  line-height: 48rpx;
}

.block {
  margin-top: 60rpx;
}

.subtitle {
  position: relative;
  font-size: 34rpx;
  line-height: 44rpx;
  font-weight: 700;
  color: #191919;
