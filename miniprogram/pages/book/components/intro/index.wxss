}

.clamp-4,.clamp-10 {
  overflow: hidden;
  white-space: normal;
  display: -webkit-box;
  -webkit-box-orient: vertical;
  -webkit-line-clamp: 4;
}

.expand {
  font-size: 26rpx;
  font-weight: 400;
  color: #818181;
  text-align: right;
}